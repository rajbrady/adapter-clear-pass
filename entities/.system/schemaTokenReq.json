{
  "$id": ".system-schemaTokenReq.json",
  "type": "object",
  "schema": "http://json-schema.org/draft-07/schema#",
  "translate": true,
  "dynamicfields": true,
  "properties": {
    "ph_request_type": {
      "type": "string",
      "description": "type of request (internal to adapter)",
      "default": "getToken",
      "enum": [
        "getToken"
      ],
      "external_name": "ph_request_type"
    },
    "username": {
      "type": "string",
      "description": "username to log in with",
      "parse": false,
      "encode": false,
      "encrypt": {
        "type": "AES",
        "key": ""
      },
      "external_name": "username"
    },
    "password": {
      "type": "string",
      "description": "password to log in with",
      "parse": false,
      "encode": false,
      "encrypt": {
        "type": "AES",
        "key": ""
      },
      "external_name": "password"
    },
    "token": {
      "type": "string",
      "description": "token returned by system",
      "parse": false,
      "encode": false,
      "placement": "BODY",
      "encrypt": {
        "type": "AES",
        "key": ""
      },
      "external_name": "token"
    }
  },
  "definitions": {}
}